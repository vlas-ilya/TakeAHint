{
  "name": "takeahint-server",
  "version": "1.0.0",
  "description": "",
  "author": "vlas-ilya",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/app.module",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "7.0.13",
    "@nestjs/core": "7.0.13",
    "@nestjs/platform-express": "7.0.13",
    "@nestjs/platform-socket.io": "7.0.13",
    "@nestjs/serve-static": "2.1.1",
    "@nestjs/websockets": "7.0.13",
    "qrcode": "1.4.4",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "uuid": "8.1.0",
    "xstate": "4.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.13",
    "@types/express": "4.17.6",
    "@types/jest": "25.1.4",
    "@types/node": "14.0.5",
    "@types/socket.io": "2.1.6",
    "@types/supertest": "2.0.9",
    "jest": "26.0.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
